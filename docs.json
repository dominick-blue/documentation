{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Project Dune",
  "colors": {
    "primary": "#1e3a5f",
    "light": "#c9a227",
    "dark": "#d4a853",
    "background": {
      "light": "#fafaf9",
      "dark": "#0c0a09"
    }
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "concepts"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/library",
              "features/notes",
              "features/drafts",
              "features/code-snippets",
              "features/exporting"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "reference/shortcuts",
              "reference/markdown",
              "reference/faq"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/yourusername/project-dune",
          "icon": "github"
        },
        {
          "anchor": "App",
          "href": "https://projectdune.app",
          "icon": "browser"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png",
    "href": "https://projectdune.mintlify.app"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@projectdune.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Open App",
      "href": "https://projectdune.app"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/yourusername/project-dune",
      "x": "https://x.com/projectdune"
    }
  },
  "modeToggle": {
    "default": "system"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "search": {
    "prompt": "Search documentation..."
  },
  "seo": {
    "indexHiddenPages": false
  }
}
